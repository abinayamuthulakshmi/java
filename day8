package day8;
STRING EMPLOYEE
public class Employee {
	int empid=98;
	String name="Akash";
	static String companyname="zoho";
	void display(int a,String name)
	{
		System.out.println(a + " "+ name +" " + companyname + " ");
	}
	public static void main(String[] args) {
		Employee obj = new Employee();
		System.out.println(obj.empid);
		System.out.println(Employee.companyname);
		obj.display(55, "Akash");
		obj.display(58, "Ashwin");
		obj.display(54, "Arjun");
		obj.display(59, "Arun");
		
	}

}

MAIN COLLEGE
package day8;

public class Main {
	int rollno=55;
	String name="Akash";
	static String college="zion";
	void display(int a,String name)
	{
		System.out.println(a + " "+ name +" " + college + " ");
	}
	public static void main(String[] args) {
		Main obj = new Main();
		System.out.println(obj.rollno);
		System.out.println(Main.college);
		obj.display(55, "Akash");
		obj.display(58, "Ashwin");
		obj.display(54, "Arjun");
		obj.display(59, "Arun");
		
	}

}

MAIN METHOD
